{
  "compilerOptions": {
      "module": "commonjs",
      "target": "es2019",
      "outDir": "dist",
      "strict": false,
      "alwaysStrict": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "resolveJsonModule": true,
      "downlevelIteration": true
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}